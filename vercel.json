{
  "version": 2,
  "name": "tailorcv",
  "builds": [
    {
      "src": "tailorcv-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/tailorcv-frontend/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/tailorcv-frontend/build/index.html"
    }
  ]
}